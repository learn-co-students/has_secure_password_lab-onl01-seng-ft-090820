<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
    <%= form_for :user do |f| %>
    <%= f.label "Username" %>
    <%= f.text_field :name %>
    <%= f.label "Password" %>
    <%= f.password_field :password %>
    <%= f.label "Password Confirmation" %>
    <%= f.password_field :password_confirmation %>
    <%= f.submit "Login" %>
<% end %>